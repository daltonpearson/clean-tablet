{
  "name": "blank-slate",
  "version": "1.0.0",
  "description": "clone of Blank Slate, a party game",
  "main": "index.js",
  "scripts": {
    "build": "webpack --env.ENV=prod --env.WS=wss://blank-slate.herokuapp.com/ws",
    "build:local": "webpack --env.ENV=prod --env.WS=ws://localhost:8000/ws",
    "cover": "go tool cover -html c.out",
    "dev": "webpack-dev-server --env.ENV=dev --env.WS=ws://localhost:8000/ws",
    "lint": "eslint --fix src/front/ && echo 'Linted!'",
    "local": "heroku local -p 8000",
    "test": "go test -v github.com/jamessouth/blank-slate/src/server/clients",
    "test:coverage": "go test -coverprofile c.out github.com/jamessouth/blank-slate/src/server/clients"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamessouth/blank-slate.git"
  },
  "author": "james south",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamessouth/blank-slate/issues"
  },
  "homepage": "https://github.com/jamessouth/blank-slate#readme",
  "dependencies": {
    "core-js": "^3.6.4",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "nyc": "^15.0.0",
    "riteway": "^6.1.1",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "style-loader": "^1.1.3",
    "tap-nirvana": "^1.1.0",
    "watch": "^1.0.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3"
  }
}
